- form_for(@domain) do |f|
  = f.error_messages
  %p
    = f.label t('domain.name')
    %br/
    = f.text_field :name
  %p
    = f.label t('domain.result_name')
    %br/
    = f.text_field :result_name
  %p
    = f.label t('domain.is_role')
    %br/
    = f.check_box :is_role
  %p
    = f.label t('domain.is_userdomain')
    %br/
    = f.check_box :is_userdomain
  %p
    = f.submit t('action.save')